<ServerManagerConfiguration>
  <ProxyGroup name="sources">
    <SourceProxy name="SOTEPSReader"
                 class="vtkEPSReader">
      <StringVectorProperty
        name="FileName"
        command="SetFileName"
        number_of_elements="1">
        <FileListDomain name="files"/>
      </StringVectorProperty>
      
      <StringVectorProperty
        name="Date"
        command="SetDate"
        number_of_elements="1"
        default_values="2008-01-01T00:00:00">     
      </StringVectorProperty>
           
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
