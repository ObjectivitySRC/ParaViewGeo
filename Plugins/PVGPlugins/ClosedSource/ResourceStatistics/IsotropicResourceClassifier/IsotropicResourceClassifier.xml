<ServerManagerConfiguration>
  <!-- Begin IsotropicResourceClassifier -->
  <ProxyGroup name="filters">
    <SourceProxy name="Isotropic Resource Classifier" class="vtkIsotropicResourceClassifier">

      <InputProperty
        name="Input"
        command="SetInputConnection"> 
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkPolyData"/>
        </DataTypeDomain>
      </InputProperty>

      <InputProperty
        name="Source"
        command="SetSourceConnection" >
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkPolyData"/>
        </DataTypeDomain>
      </InputProperty>

      <DoubleVectorProperty
        name="Measured Distance"
        command="SetDistanceMeasured"
        number_of_elements="1"
        default_values="50.0">
        <DoubleRangeDomain name="range" min="0.0"/>
      </DoubleVectorProperty>

      <DoubleVectorProperty
        name="Indicated Distance"
        command="SetDistanceIndicated"
        number_of_elements="1"
        default_values="100.0">
        <DoubleRangeDomain name="range" min="0.0"/>
      </DoubleVectorProperty>

      <DoubleVectorProperty
        name="Inferred Distance"
        command="SetDistanceInferred"
        number_of_elements="1"
        default_values="150.0">
        <DoubleRangeDomain name="range" min="0.0"/>
      </DoubleVectorProperty>

    </SourceProxy>
  </ProxyGroup>
  <!-- End IsotropicResourceClassifier -->
</ServerManagerConfiguration>