
# Base all directories relative to the location of the ParaViewConfig.cmake file.
GET_FILENAME_COMPONENT(ParaView_INSTALL_PREFIX "${ParaView_DIR}@REL_REF@" ABSOLUTE)

# Version information for ParaView
SET(PARAVIEW_VERSION_MAJOR "@PARAVIEW_VERSION_MAJOR@")
SET(PARAVIEW_VERSION_MINOR "@PARAVIEW_VERSION_MINOR@")
SET(PARAVIEW_VERSION_PATCH "@PARAVIEW_VERSION_PATCH@")
SET(PARAVIEW_VERSION_FULL    "@PARAVIEW_VERSION_FULL@")

# Locations for the source and binary dirs.
# I am not sure ParaView_SOURCE_DIR should be exported since it won't be
# available for development-install
SET(ParaView_CMAKE_DIR "@ParaView_CMAKE_DIR@")
SET(ParaView_BINARY_DIR "@ParaView_BINARY_DIR@")

# Some core ParaView settings.
SET(PARAVIEW_BUILD_SHARED_LIBS "@PARAVIEW_BUILD_SHARED_LIBS@")
SET(PARAVIEW_ENABLE_PYTHON "@PARAVIEW_ENABLE_PYTHON@")
SET(PARAVIEW_BUILD_QT_GUI "@PARAVIEW_BUILD_QT_GUI@")

IF (PARAVIEW_ENABLE_PYTHON)
  SET (PARAVIEW_PYTHON_INCLUDE_DIR "@PYTHON_INCLUDE_DIR@")
ENDIF()

SET(PARAVIEW_USE_FILE "@PARAVIEW_USE_FILE@")
SET(PARAVIEW_VTK_DIR "@PARAVIEW_VTK_DIR@")
SET(PARAVIEW_QT_QMAKE_EXECUTABLE "@PARAVIEW_QT_QMAKE_EXECUTABLE@")
SET(ParaView_QT_DIR "@ParaView_QT_DIR@")

# Locate to extra CMake modules.
#SET(PARAVIEW_CMAKE_EXTENSIONS "@PARAVIEW_CMAKE_EXTENSIONS@")

SET(VTK_WRAP_ClientServer_EXE "@VTK_WRAP_ClientServer_EXE_CONFIG@")
SET(PARAVIEW_INCLUDE_DIRS "@PARAVIEW_INCLUDE_DIRS@")
SET(PARAVIEW_LIBRARY_DIRS "@PARAVIEW_LIBRARY_DIRS@")

SET(PARAVIEW_GUI_INCLUDE_DIRS "@PARAVIEW_GUI_INCLUDE_DIRS@")
SET(PARAVIEW_KWSYS_INCLUDE_DIRS "@PARAVIEW_KWSYS_INCLUDE_DIRS@")

# Path to various paraview executables needed for testing.
SET(PARAVIEW_BATCH_EXECUTABLE "@PARAVIEW_BATCH_EXECUTABLE@")
SET(PARAVIEW_PYTHON_EXECUTABLE "@PARAVIEW_PYTHON_EXECUTABLE@")
SET(PARAVIEW_SERVER_EXECUTABLE "@PARAVIEW_SERVER_EXECUTABLE@")
SET(PARAVIEW_DATA_SERVER_EXECUTABLE "@PARAVIEW_DATA_SERVER_EXECUTABLE@")
SET(PARAVIEW_RENDER_SERVER_EXECUTABLE "@PARAVIEW_RENDER_SERVER_EXECUTABLE@")
SET(PARAVIEW_SMTESTDRIVER_EXECUTABLE "@PARAVIEW_SMTESTDRIVER_EXECUTABLE@")

SET(PARAVIEW_PROCESS_XML_EXECUTABLE "@PARAVIEW_PROCESS_XML_EXECUTABLE@")
SET(PARAVIEW_SERVER_MANAGER_RESOURCES_DIRECTORY "@PARAVIEW_SERVER_MANAGER_RESOURCES_DIRECTORY@")
SET(VTKCS_CONFIG_DIR "@VTKCS_CONFIG_DIR@")

SET(PARAVIEW_HDF5_LIBRARIES "@PARAVIEW_HDF5_LIBRARIES@")

# The ParaView targets file.
IF(NOT PV_INSTALL_EXPORT_NAME AND NOT TARGET vtkPVCommon
   AND EXISTS "@PARAVIEW_TARGETS_FILE@")
  INCLUDE("@PARAVIEW_TARGETS_FILE@")
ENDIF()

SET(VTK_MAKE_INSTANTIATOR "@VTK_MAKE_INSTANTIATOR@")
SET(VTK_ENCODESTRING_EXE "@VTK_ENCODESTRING_EXE@")

# The locations/prefixes where this version of ParaView would be installed.
SET (PARAVIEW_INSTALL_LIB_DIR "@PV_INSTALL_LIB_DIR@")

SET(PARAVIEW_ENABLE_COPROCESSING "@PARAVIEW_ENABLE_COPROCESSING@")
IF(PARAVIEW_ENABLE_COPROCESSING)
  SET(CoProcessing_DIR "@CoProcessing_BINARY_DIR@")
ENDIF(PARAVIEW_ENABLE_COPROCESSING)

SET(PARAVIEW_USE_VISITBRIDGE "@PARAVIEW_USE_VISITBRIDGE@")
IF(PARAVIEW_USE_VISITBRIDGE)
  SET(VISIT_CMAKE_DIR "@VISITBRIDGE_CMAKE_DIR@")
  SET(VISITBRIDGE_USE_FILE "@VISITBRIDGE_USE_FILE@")
ENDIF(PARAVIEW_USE_VISITBRIDGE)
